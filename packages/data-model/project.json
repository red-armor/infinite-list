{
  "name": "data-model",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/data-model/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/data-model",
        "main": "packages/data-model/src/index.ts",
        "tsConfig": "packages/data-model/tsconfig.lib.json",
        "assets": ["packages/data-model/*.md"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
        "options": {
          "config": "packages/data-model/vite.config.ts"
        }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    }
  }
}
